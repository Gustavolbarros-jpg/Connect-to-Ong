generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user_verifications {
  id            String    @id @default(uuid()) @db.Uuid
  user_id       String    @db.Uuid
  unique_string Bytes
  salt          Bytes?
  created_at    DateTime? @default(now()) @db.Timestamptz(6)
  expires_at    DateTime  @db.Timestamptz(6)
  user          users     @relation("UserVerifications", fields: [user_id], references: [id])
}

model users {
  id            String               @id @default(uuid()) @db.Uuid
  fullname      String               @db.VarChar(255)
  email         String               @unique @db.VarChar(255)
  institution   String?              @db.VarChar(255)
  password      String               @db.VarChar(255)
  salt          String               @db.VarChar(255)
  verified      Boolean?             @default(false)
  description   String?
  photoUrl      String?
  projetos      projetos[]           @relation("UserProjetos")
  verifications user_verifications[] @relation("UserVerifications")
}

model projetos {
  id                    String   @id @default(uuid()) @db.Uuid
  nome_projeto          String   @db.VarChar(255)
  area_interesse        String   @db.VarChar(255)
  soft_skills           String   @db.VarChar(500)
  quantidade_alunos     Int
  descricao_projeto     String
  professores_atrelados String   @db.VarChar(500)
  horas_extensao        Int
  tempo_previsto        Int
  ong_selecionada       String?  @db.VarChar(255)
  categoria_ong         String?  @db.VarChar(255)
  status                String   @default("Aguardando Conexão") @db.VarChar(100)
  created_at            DateTime @default(now()) @db.Timestamptz(6)
  updated_at            DateTime @updatedAt @db.Timestamptz(6)
  user_id               String   @db.Uuid
  user                  users    @relation("UserProjetos", fields: [user_id], references: [id])
}

model Ong {
  id                                                             Int     @id @default(autoincrement())
  nome_ong                                                       String? @db.VarChar(255)
  motivo_criacao                                                 String?
  sobre                                                          String?
  responsavel_contato                                            String? @db.VarChar(255)
  cnpj                                                           String? @db.VarChar(20)
  quantidade_assistidos                                          String? @db.VarChar(255)
  metas                                                          String?
  causa_organizacao                                              String? @db.VarChar(255)
  area                                                           String?
  match_area                                                     String?
  possui_voluntarios                                             String? @db.VarChar(100)
  possui_funcionarios                                            String? @db.VarChar(100)
  funcionamento                                                  String? @db.VarChar(255)
  endereco                                                       String?
  rpa                                                            String? @db.VarChar(10)
  contato_divulgacao                                             String? @db.VarChar(100)
  redes_sociais                                                  String?
  produto_social                                                 String?
  mensagem_convite                                               String?
  serve_refeicao                                                 String?
  Nome_da_ONG_Projeto                                            String? @map("Nome da ONG/Projeto") @db.VarChar(128)
  Motivo_da_cria__o_do_projeto_                                  String? @map("Motivo da criação do projeto\n") @db.VarChar(4096)
  Respons_vel_para_contato                                       String? @map(" Responsável para contato") @db.VarChar(2048)
  CNPJ_                                                          String? @map("CNPJ ") @db.VarChar(50)
  Quantidade_de_pessoas_assistidas_beneficiadas_atrav_s_do_traba String? @map("Quantidade de pessoas assistidas/beneficiadas através do traba") @db.VarChar(512)
  Metas_de_curto__m_dio_e_longo_prazo                            String? @map("Metas de curto, médio e longo prazo") @db.VarChar(4096)
  Causa_da_Organiza__o_                                          String? @map("Causa da Organização\n") @db.VarChar(1024)
  rea                                                            String? @map("área") @db.VarChar(128)
  Match                                                          String? @db.VarChar(512)
  Objetivos_de_Desenvolvimento_Sustent_vel_ODS_que_a_institui_   String? @map("Objetivos de Desenvolvimento Sustentável-ODS que a instituiç") @db.VarChar(1024)
  Atividades_que_o_projeto_desenvolve_                           String? @map("Atividades que o projeto desenvolve\n") @db.VarChar(4096)
  Possui_Volunt_rios_fixos__Se_sim__quantos_s_o_                 String? @map(" Possui Voluntários fixos? Se sim, quantos são?") @db.VarChar(512)
  A_sua_organiza__o_possui_funcion_rios__Se_sim__quantos_s_o_    String? @map("A sua organização possui funcionários?\nSe sim, quantos são?") @db.VarChar(128)
  Funcionamento__da_organiza__o                                  String? @map("Funcionamento  da organização") @db.VarChar(256)
  endere_o                                                       String? @map("endereço") @db.VarChar(256)
  Contato_para_divulga__o                                        String? @map("Contato para divulgação") @db.VarChar(256)
  Espa_o_para_informarem_as_redes_sociais_da_sua_Organiza__o__   String? @map("Espaço para informarem\nas redes sociais da sua Organização:\n") @db.VarChar(512)
  Voc_s_possuem_algum_produto_social__Explicando_melhor__Voc_s_  String? @map("Vocês possuem algum produto social? Explicando melhor: Vocês ") @db.VarChar(1024)
  Qual_mensagem_voc__mandaria_para_as_pessoas_que_ainda_n_o_co   String? @map(" Qual mensagem você mandaria para as pessoas que ainda não co") @db.VarChar(2048)
  A_sua_entidade_serve_refei__es__se_sim__a_cozinha_que_prepara  String? @map("A sua entidade serve refeições? se sim, a cozinha que prepara") @db.VarChar(512)

  @@map("ongs")
}
